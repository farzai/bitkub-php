{
    "name": "farzai/bitkub",
    "description": "The bitkub api wrapper",
    "keywords": [
        "farzai",
        "bitkub"
    ],
    "homepage": "https://github.com/farzai/bitkub-php",
    "license": "MIT",
    "authors": [
        {
            "name": "parsilver",
            "email": "parkorn@farzai.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "farzai/support": "^1.2",
        "farzai/transport": "^1.2",
        "phrity/websocket": "^3.0"
    },
    "require-dev": {
        "pestphp/pest": "^3.4",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Farzai\\Bitkub\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Farzai\\Bitkub\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
